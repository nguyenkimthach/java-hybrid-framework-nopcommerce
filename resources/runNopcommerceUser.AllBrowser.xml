<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite parallel="tests" name="Nopcommerce - User" thread-count="6">

	<listeners>
		<listener class-name="reportConfig.ExtentTestListenerV5" />
	</listeners>

	<!-- Global -->
	<parameter name="envName" value="local" />


	<test name="Run on Headless Chrome">
		<parameter name="browserName" value="H_Chrome" />
		<classes>
			<class name="com.nopcommerce.user.User_01_Register" />
		</classes>
	</test>

	<test name="Run on Headless Firefox">
		<parameter name="browserName" value="H_Firefox" />
		<classes>
			<class name="com.nopcommerce.user.User_01_Register" />
		</classes>
	</test>

	<test name="Run on Firefox">
		<parameter name="browserName" value="Firefox" />
		<classes>
			<class name="com.nopcommerce.user.User_01_Register" />
		</classes>
	</test>

	<test name="Run on Chrome">

		<parameter name="browserName" value="Chrome" />
		<classes>
			<class name="com.nopcommerce.user.User_01_Register" />
		</classes>
	</test>

	<test name="Run on Edge">
		<parameter name="browserName" value="Edge" />
		<classes>
			<class name="com.nopcommerce.user.User_01_Register" />
		</classes>
	</test>

	<test name="Run on IE">
		<parameter name="browserName" value="IE" />
		<classes>
			<class name="com.nopcommerce.user.User_01_Register" />
		</classes>
	</test>

	<test name="Run on Coccoc">
		<parameter name="browserName" value="Coccoc" />
		<classes>
			<class name="com.nopcommerce.user.User_01_Register" />
		</classes>
	</test>
</suite> 
 		